{
  "expo": {
    "name": "VCrypto",
    "slug": "VCrypto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vcrypto",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F68C1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "crypto",
      "buildNumber": "1.0.3",
      "supportsTablet": true
    },
    "android": {
      "package": "crypto",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2F68C1"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
  }
}
